      <div class="col">
        <p>{{ .Title }}</p>
      </div>
      <div class="col">
        <p>{{.AddedBy.FirstName}} {{.AddedBy.LastName}}</p> 
      </div>
    </div>
  {{ end }}
  </div>
</div>
{{ end }}
